{% extends 'layout.html.twig' %}

{% block stylesheets %}
    {{parent()}}
{%endblock%}

{% block body %}
    {{parent()}}
    
    <div class='container-fluid'> 
        <div class='row'>
            <div class='col-md-12'>
                <div  class='col-md-12'>
                    <h2>Informations générales</h2>
                    
                    <p>Nom : {{fournisseur.nom}}</p>
                    <p>Numéro de téléphone : {{fournisseur.numTelephone}}</p>
                    <p>Mail : {{fournisseur.mail}}</p>
                    <p>Site web  : {{fournisseur.siteWeb}}</p>
                    <p>Adresse : {{fournisseur.adresse}}</p>
                    <p>Observations : {{fournisseur.observations}}</p>
                </div>
            </div>
        </div>
       <div id ='btRetour' >
            <a href="{{ path('menu_fournisseurs')}}">
                <input type="button"  class="btn btn-primary" value="Retour">
            </a>
        </div>
    </div>
{%endblock%}
